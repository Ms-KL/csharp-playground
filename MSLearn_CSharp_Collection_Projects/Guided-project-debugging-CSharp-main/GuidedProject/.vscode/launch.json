{
    "version": "0.2.0",
    "configurations": [
        {
            // Use IntelliSense to find out which attributes exist for C# debugging
            // Use hover for the description of the existing attributes
            // For further information visit https://github.com/dotnet/vscode-csharp/blob/main/debugger-launchjson.md
            "name": "launch Starter",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildStarter",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/Starter/bin/Debug/net7.0/Starter.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Starter",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "integratedTerminal", // updated due to readline()
            "stopAtEntry": false
        },
        {
            // Use IntelliSense to find out which attributes exist for C# debugging
            // Use hover for the description of the existing attributes
            // For further information visit https://github.com/dotnet/vscode-csharp/blob/main/debugger-launchjson.md
            "name": "launch Final",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildFinal",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/Final/bin/Debug/net7.0/Final.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Final",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "integratedTerminal", // updated due to readline()
            "stopAtEntry": false
